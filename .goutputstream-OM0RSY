
#include "install.c"
#include <string.h>

int main ( int argc, char **argv ) {
	printf ( "XGET: the super simple, cross-platform installer\n" );
	printf( "%s\n", argv[1] );
	if ( strcmp (argv[1], "install") != 0 ) {
		printf ( "Install %s\n", argv[2] );
		install ( argv[2] );
	} else {
		printf("Not installing :( \n");
	}
}
